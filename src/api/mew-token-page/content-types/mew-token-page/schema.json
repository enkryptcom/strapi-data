      "required": true
    },
    "whatIsToken": {
      "type": "richtext",
      "required": true
    },
    "coingeckoId": {
      "type": "string",
      "required": true
    },
    "icon": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "required": true
