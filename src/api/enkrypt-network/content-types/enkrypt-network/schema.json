{
  "kind": "collectionType",
  "collectionName": "enkrypt_networks",
  "info": {
    "singularName": "enkrypt-network",
    "pluralName": "enkrypt-networks",
    "displayName": "enkrypt-network",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true,
      "minLength": 3
    },
    "path": {
      "type": "uid",
      "required": true
    },
    "pageTitle": {
      "type": "string",
      "required": true
    },
    "pageDescription": {
      "type": "richtext",
      "required": true
    },
    "aboutEnkrypt": {
      "type": "richtext",
      "required": true
    },
    "networkInfo": {
      "type": "richtext",
      "required": true
    },
    "icon": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "aboutEnkryptTitle": {
      "type": "string",
      "default": "About Enkrypt",
      "required": true
    },
    "networkInfoTitle": {
      "type": "string",
      "default": "What is [network Name]?",
      "required": true
    }
  }
}
